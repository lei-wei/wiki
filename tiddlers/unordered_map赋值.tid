class Solution
{
private:
    unordered_map<char, int> hashmap = {
        {'I', 1},
        {'V', 5},
        {'X', 10},
        {'L', 50},
        {'C', 100},
        {'D', 500},
        {'M', 1000},
    };

public:
    int romanToInt(string s)
    {
        int res = 0;
        for (size_t i = 0; i < s.size(); i++)
        {
            if (i < s.size() - 1 && hashmap[s[i]] < hashmap[s[i + 1]]) //出现左边的情况
            {
                res += hashmap[s[i + 1]] - hashmap[s[i]];
                i++;
            }
            else //正常相加
            {
                res += hashmap[s[i]];
            }
        }

        return res;
    }
};
